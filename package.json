{
  "name": "file-system-store",
  "version": "1.1.2",
  "main": "index.js",
  "description": "File System Store for MongoPortable, a portable MongoDB module (https://github.com/eastolfi/MongoPortable)",
  "author": "Eduardo Astolfi <eastolfi91@gmail.com>",
  "license": "MIT",
  "scripts": {
    "docs": "gulp doc",
    "build": "tsc -p ./tsconfig.json",
    "build.watch": "tsc -p ./tsconfig.json -w",
    "test": "mocha test/**/*.spec.ts",
    "test.coverage": "nyc mocha test/**/*.spec.ts",
    "test.coveralls": "nyc mocha test/**/*.spec.ts | coveralls",
    "generate": "npm run test && npm run build && npm run docs",
    "release": "gulp release",
    "depcheck": "depcheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EastolfiWebDev/FileSystemStore.git"
  },
  "homepage": "https://github.com/EastolfiWebDev/FileSystemStore#readme",
  "bugs": {
    "url": "https://github.com/EastolfiWebDev/FileSystemStore/issues"
  },
  "nyc": {
    "include": [
      "src/*.{js,ts,tsx}"
    ],
    "exclude": [
      "**/node_modules/**",
      "src/**/*.d.ts"
    ],
    "extension": [
      ".js",
      ".ts",
      ".tsx"
    ],
    "require": [
      "source-map-support/register",
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
	  "text-summary"
    ],
    "produceSourceMap": true,
    "sourceMap": true,
    "instrument": true,
    "all": true,
    "report-dir": "test/coverage"
  },
  "dependencies": {
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.14",
    "chai": "^4.1.0",
    "file-system": "^2.2.2",
    "jsw-logger": "^1.2.4",
    "lodash": "^4.17.4",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "promise": "^8.0.1",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.2.0",
    "tsconfig-paths": "^2.2.0",
    "typescript": "^2.4.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "depcheck": "^0.6.7",
    "gulp": "^3.9.1",
    "gulp-conventional-changelog": "^1.1.6",
    "gulp-coveralls": "^0.1.4",
    "gulp-jsdoc-to-markdown": "^1.2.2",
    "gulp-rename": "^1.2.2",
    "gulp-typedoc": "^2.1.0",
    "jsdoc-to-markdown": "^3.0.0",
    "mongo-portable": "^1.4.0",
    "require-dir": "^0.3.2",
    "rimraf": "^2.6.2",
    "run-sequence": "^2.2.0",
    "typedoc": "^0.8.0"
  }
}
