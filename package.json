{
  "name": "file-system-store",
  "version": "1.1.2",
  "description": "File System Store for MongoPortable, a portable MongoDB module (https://github.com/eastolfi/MongoPortable)",
  "author": "Eduardo Astolfi <eastolfi91@gmail.com>",
  "keywords": [
    "mongo",
    "portable",
    "ddbb",
    "no",
    "sql",
    "relational",
    "store",
    "node",
    "file",
    "system",
    "persistence",
    "store"
  ],
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "dependencies": {
    "file-system": "^2.2.1",
    "jsw-logger": "^1.2.3",
    "lodash": "^4.11.2",
    "mongo-portable": "^1.3.0",
    "promise": "^7.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "chai": "^4.1.2",
    "conventional-github-releaser": "^1.1.3",
    "coveralls": "^2.11.9",
    "del": "^2.2.2",
    "depcheck": "^0.6.7",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.5.1",
    "gulp-conventional-changelog": "^1.1.0",
    "gulp-git": "^1.12.0",
    "gulp-jsdoc-to-markdown": "^1.2.2",
    "gulp-mocha": "^3.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-typedoc": "^2.0.1",
    "gulp-typescript": "^3.1.4",
    "gulp-util": "^3.0.8",
    "jscoverage": "^0.6.0",
    "jsdoc-to-markdown": "^2.0.1",
    "minimist": "^1.2.0",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "mongo-portable": "^1.3.0",
    "nyc": "^11.2.1",
    "remap-istanbul": "^0.9.5",
    "require-dir": "^0.3.1",
    "rimraf": "^2.5.2",
    "run-sequence": "^1.2.2",
    "source-map-support": "^0.4.18",
    "ts-node": "^3.3.0",
    "tsconfig-paths": "^2.3.0",
    "typedoc": "^0.5.3",
    "typescript": "^2.1.4"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "build.watch": "tsc -p ./tsconfig.json -w",
    "test.old": "gulp test",
    "test": "mocha",
    "test.coverage": "nyc npm run test",
    "test.coverage2": "nyc gulp test:app2",
    "test.coveralls": "nyc npm run test | coveralls",
    "generate": "npm run test && npm run build",
    "release": "gulp release",
    "depcheck": "depcheck",
    "test2": "node ./node_modules/mocha/bin/mocha -r ts-node/register './test/**/*.spec.ts'",
    "cover2": "node ./node_modules/nyc/bin/nyc.js ./node_modules/mocha/bin/mocha -r ts-node/register './test/unit/FileSystemStore.specs.ts'",
    "clear2": "rm -rf .nyc_output/ && rm -rf coverage/ && rm -rf node_modules/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EastolfiWebDev/FileSystemStore.git"
  },
  "homepage": "https://github.com/EastolfiWebDev/FileSystemStore#readme",
  "bugs": {
    "url": "https://github.com/EastolfiWebDev/FileSystemStore/issues"
  },
  "license": "MIT",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/node_modules/**",
      "src/**/*.d.ts"
    ],
    "require": [
      "source-map-support/register",
      "ts-node/register"
    ],
    "produceSourceMap": true,
    "sourceMap": true,
    "instrument": true,
    "all": true,
    "reporter": [
      "html",
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  }
}
